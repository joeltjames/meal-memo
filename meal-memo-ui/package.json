{
    "name": "meal-memo-ui",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "cy:run-ci": "cypress run --config video=false",
        "e2e": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && npm run cy:run\" --kill-others --success first",
        "e2e-gui": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && npm run cy:open\" --kill-others --success first",
        "e2e-ci": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && npm run cy:run-ci\" --kill-others --success first"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.1.0",
        "@angular/cdk": "~11.1.0",
        "@angular/common": "~11.1.0",
